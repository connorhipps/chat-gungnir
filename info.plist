				<key>script</key>
				<string>#!/bin/bash

# Get the contents of the clipboard
clipboard=$(pbpaste)

# Check if Brave is running
is_brave_running=$(osascript -e 'if application "Brave Browser" is running then return 1 else return 0 end if')

# If Brave is not running, open it
if [ $is_brave_running -eq 0 ]; then
    open -a "Brave Browser"
    sleep 5 # Wait for Brave to launch
fi

# Open the Chat OpenAI website in Brave
open -a "Brave Browser" https://chat.openai.com/

# Wait for the page to load
sleep 3

# Focus on the message input box using JavaScript
osascript -e 'tell application "Brave Browser" to activate' -e 'tell application "Brave Browser" to execute javascript "document.querySelectorAll(\"[data-slate-editor]\")[0].focus()"'

# Paste the combined message into the input box
osascript -e 'tell application "Brave Browser" to activate' -e 'tell application "System Events" to keystroke "v" using command down'

# Wait a bit for the text to be pasted
sleep 0.5</string>
